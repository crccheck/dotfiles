set tabstop=4
set shiftwidth=4
set expandtab
set smarttab
set ignorecase
set smartcase
set incsearch
set ai
set backup
set guifont=Monospace\ 8
set number
set title
filetype plugin on
syn on
set backupdir=~/.vim-tmp,~/.tmp,~/tmp,/var/tmp,/tmp
set directory=~/.vim-tmp,~/.tmp,~/tmp,/var/tmp,/tmp
set nowrap
