"*":
  "autocomplete-python":
    caseInsensitiveCompletion: false
  "bracket-matcher":
    autocompleteBrackets: false
  core:
    disabledPackages: [
      "metrics"
      "project-manager"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".cache"
      ".DS_Store"
      ".git"
      "*.pyc"
      ".ipynb_checkpoints"
      ".tox"
      "__pycache__"
      "coverage"
      "node_modules"
    ]
    packagesWithKeymapsDisabled: [
      "emmet"
      "autocomplete-python"
      "bracket-matcher"
      "python-yapf"
    ]
    themes: [
      "atom-dark-ui"
      "atom-dark-syntax"
    ]
  editor:
    autoIndentOnPaste: false
    fontSize: 12
    invisibles:
      eol: false
    preferredLineLength: 79
    showInvisibles: true
  emmet: {}
  "exception-reporting":
    userId: "92efa8a2-ea08-edd4-697a-efdac8b09d9c"
  "git-diff": {}
  linter:
    lintOnFly: false
    showErrorPanel: false
  "linter-eslint":
    globalNodePath: "/usr/local"
    useGlobalEslint: true
  "linter-flake8":
    maxComplexity: 100
  "python-yapf":
    checkOnSave: false
    yapfPath: "/usr/local/bin/yapf"
  tabs:
    enableVcsColoring: true
    usePreviewTabs: true
  "tree-view":
    hideIgnoredNames: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
  welcome:
    showOnStartup: false
  wrap: {}
".apache-config.mod_perl.source":
  editor:
    preferredLineLength: 79
".apache-config.source":
  editor:
    preferredLineLength: 79
".console.python.text":
  editor:
    preferredLineLength: 79
".python.regexp.source":
  editor:
    preferredLineLength: 79
".python.source":
  editor:
    preferredLineLength: 79
".python.text.traceback":
  editor:
    preferredLineLength: 79
".scala.source":
  editor:
    preferredLineLength: 79
